#include <iostream>
#include <cmath>

using namespace std;
int main()
{
	int cislo;

	while(true) {
	    cin >> cislo;
		if(cislo <= 0) {
			cout << "neni prvocislo";
			break;
		}
		bool jePrvocislo = true;
		for (int i = 2; i <= sqrt(cislo); i++) {
			if(cislo % i == 0) {
                jePrvocislo = false;
			}
		}
        if(jePrvocislo){
            cout <<"je prvocislo";
            continue;
        }else{
            cout << "neni prvocislo";
            continue;
        }
	}
}
